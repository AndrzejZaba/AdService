{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "cldrjs@0.5.5",
      "destination": "wwwroot/lib/cldrjs/",
      "files": [
        "dist/cldr.js",
        "dist/cldr/event.js",
        "dist/cldr/supplemental.js"
      ]
    },
    {
      "library": "globalize@1.7.0",
      "destination": "wwwroot/lib/globalize/",
      "files": [
        "globalize.js",
        "globalize/number.js",
        "globalize/date.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-globalize-fix@0.1.1",
      "destination": "wwwroot/lib/jquery-validation-globalize-fix/",
      "files": [
        "jquery.validate.globalize.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "cldr-core@40.0.0",
      "destination": "wwwroot/lib/cldr-core/",
      "files": [
        "supplemental/likelySubtags.json",
        "supplemental/numberingSystems.json",
        "supplemental/plurals.json",
        "supplemental/ordinals.json",
        "supplemental/calendarData.json",
        "supplemental/timeData.json",
        "supplemental/weekData.json"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "cldr-dates-modern@40.0.0",
      "destination": "wwwroot/lib/cldr-dates-modern/",
      "files": [
        "main/en/ca-gregorian.json",
        "main/en/timeZoneNames.json",
        "main/pl/ca-gregorian.json",
        "main/pl/timeZoneNames.json"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "cldr-numbers-modern@40.0.0",
      "destination": "wwwroot/lib/cldr-numbers-modern/",
      "files": [
        "main/en/numbers.json",
        "main/en/currencies.json",
        "main/pl/currencies.json",
        "main/pl/numbers.json"
      ]
    },
    {
      "library": "font-awesome@6.5.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    },
    {
      "library": "inputmask@4.0.9",
      "destination": "wwwroot/lib/inputmask/",
      "files": [
        "jquery.inputmask.bundle.min.js"
      ]
    },
    {
      "library": "tempusdominus-bootstrap-4@5.39.0",
      "destination": "wwwroot/lib/tempusdominus-bootstrap-4/",
      "files": [
        "js/tempusdominus-bootstrap-4.min.js",
        "css/tempusdominus-bootstrap-4.min.css"
      ]
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/",
      "files": [
        "moment.min.js",
        "locale/en-gb.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "css/select2.min.css",
        "js/select2.full.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "select2-bootstrap-5-theme@1.3.0",
      "destination": "wwwroot/lib/select2-bootstrap-5-theme/",
      "files": [
        "select2-bootstrap-5-theme.min.css"
      ]
    }
  ]
}